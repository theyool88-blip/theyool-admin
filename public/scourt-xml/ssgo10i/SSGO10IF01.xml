};

/**
 *********************************************************************************************************
 * Submission Event Handler
 *********************************************************************************************************
 */
/**
 * submit 이벤트
 *
 * ============ ============ ==========================
 *  Date         Author       Description
 * ============ ============ ==========================
 * 2022.05.10.   최연구       [NELS]최초생성
 * ============ ============ ==========================
 */
scwin.sbm_search_submit = function(e) {
}; 

/**
 * submitdone 이벤트
 *
 * ============ ============ ==========================
 *  Date         Author       Description
 * ============ ============ ==========================
 * 2022.05.10.   최연구       [NELS]최초생성
 * ============ ============ ==========================
 */
scwin.sbm_search_submitdone = function(e) {
    var csRsltData = e.responseJSON.data;
    
    // 새로 열릴 화면에 데이터를 전달 (dataObject를 통해 전달 가능)         
    var wfObj = {"dataObject": {"type" : "json", "name" : "paramData", "data" : null,}}; 
    
    // 가공처리되는 정보 ------------------------------------------------
    scwin.ufn_setDataArilPrcs();
    
    // 심급내용
    if(!ssgo.isLenZero(csRsltData.dlt_inscrtDtsLst)) {
        wfObj.dataObject.data = csRsltData.dlt_inscrtDtsLst;
        wfInscrtDtsLst.setSrc(ssgo.getCtxtPathUrl("/ui/ssgo003/SSGO003F11.xml"), wfObj);    
    }

    // 최근기일내용
    wfObj.dataObject.data = csRsltData.dlt_rcntDxdyLst;
    wfRcntDxdyLst.setSrc(ssgo.getCtxtPathUrl("/ui/ssgo003/SSGO003F32.xml"), wfObj);  

    // 최근 제출서류 접수내용
    wfObj.dataObject.data = csRsltData.dlt_rcntSbmsnDocmtLst;
    wfRcntSbmsnDocmtLst.setSrc(ssgo.getCtxtPathUrl("/ui/ssgo003/SSGO003F40.xml"), wfObj);    

    // 관련사건내용
    if(!ssgo.isLenZero(csRsltData.dlt_reltCsLst)) {
        wfObj.dataObject.data = csRsltData.dlt_reltCsLst;
        wfReltCsLst.setSrc(ssgo.getCtxtPathUrl("/ui/ssgo003/SSGO003F50.xml"), wfObj);    
    }

    // 보호처분내용
    if(!ssgo.isLenZero(csRsltData.dlt_prtctDspsCttLst)) {
        wfObj.dataObject.data = csRsltData.dlt_prtctDspsCttLst;
        wfPrtctDspsLst.setSrc(ssgo.getCtxtPathUrl("/ui/ssgo003/SSGO003FG0.xml"), wfObj);        
    }  

    // 임시조치내용
    if(!ssgo.isLenZero(csRsltData.dlt_tmprtDspsCttLst)) {
        wfObj.dataObject.data = csRsltData.dlt_tmprtDspsCttLst;
        wfTmprActnLst.setSrc(ssgo.getCtxtPathUrl("/ui/ssgo003/SSGO003FH0.xml"), wfObj);    
    }

    // 사건명내용
    if(!ssgo.isLenZero(csRsltData.dlt_crimeNmCttLst)) {
        wfObj.dataObject.data = csRsltData.dlt_crimeNmCttLst;
        wfCsNmLst.setSrc(ssgo.getCtxtPathUrl("/ui/ssgo003/SSGO003FF0.xml"), wfObj);    
    }

    // 행위자(당사자)내용
    if(!ssgo.isLenZero(csRsltData.dlt_actorCttLst)) {
        wfObj.dataObject.data = csRsltData.dlt_actorCttLst;
        wfBtprtCttLst.setSrc(ssgo.getCtxtPathUrl("/ui/ssgo003/SSGO003F69.xml"), wfObj);    
    }

    // 소송관계인내용
    if(!ssgo.isLenZero(csRsltData.dlt_lwstRltnrCttLst)) {
        wfObj.dataObject.data = csRsltData.dlt_lwstRltnrCttLst;
        wfLwstRltnrCttLst.setSrc(ssgo.getCtxtPathUrl("/ui/ssgo003/SSGO003F80.xml"), wfObj);    
    }

    // 사건검색 후  callback
    ssgo.callbackCsSrchDetail(scwin.param, e);
    
};    

scwin.sbm_search_submiterror = function(e) {
    // 사건검색 후  callback    
    ssgo.callbackCsSrchDetail(scwin.param, e);
};


/**
 *********************************************************************************************************
 * Event Handler
 *********************************************************************************************************
 */

/**
 *********************************************************************************************************
 * Validate 
 *********************************************************************************************************
 */

/**
 *********************************************************************************************************
 * User Function
 *********************************************************************************************************
 */
/**
 * 화면제어 및 가공되는 데이터를 처리한다.
 *
 * ============ ============ ==========================
 *  Date         Author       Description
 * ============ ============ ==========================
 * 2022.05.10.   최연구       [NELS]최초생성
 * ============ ============ ==========================
 */
scwin.ufn_setDataArilPrcs = function() {
    
    // 재판부 fulltext구성
    var txtjdbnNm = dma_csBasCtt.get("jdbnNm");
    if("000100" != dma_csBasCtt.get("cortCd") && !scutilo.isEmpty(dma_csBasCtt.get("cfupMarkNm"))) {
        txtjdbnNm += " (" + dma_csBasCtt.get("cfupMarkNm") + ")";        
    }
    if(!scutilo.isEmpty(dma_csBasCtt.get("jdbnTelno"))) {
        txtjdbnNm += " (전화:" + dma_csBasCtt.get("jdbnTelno") + ")";
    }
    txt_jdbnNm.setValue(txtjdbnNm);
    
    // 종국결과 fulltext구성
    var csUltmtYmdFmt = (!scutilo.isEmpty(dma_csBasCtt.get("csUltmtYmd"))) ? $p.getFormattedDate(scdateo.getDateObj(dma_csBasCtt.get("csUltmtYmd")), 'yyyy.MM.dd') : "";
    if("000100" == dma_csBasCtt.get("cortCd") && dma_csBasCtt.get("ultmtDvsCd") == "232") {
        txt_ultmtDvsNm.setValue(csUltmtYmdFmt + " 상고기각판결");
    } else {
        txt_ultmtDvsNm.setValue(csUltmtYmdFmt + " " + dma_csBasCtt.get("crmcsUltmtDvsNm"));
    }
    
    // [20240724, 형사전자소송]
    // 소송구분코드 조회 추가.
    // 사건명 좌측에 "[전자]" 표시를 기본으로, 소송구분코드 <> '2'(전자소송)이면 미표시.

    if (dma_csBasCtt.get("lwstDvsCd") != "2") {
        txt_isElctn.hide();
    }

    // 보존,폐기 view
    if(!scutilo.isEmpty(dma_csBasCtt.get("prsvCtt"))) {
        if(dma_csBasCtt.get("prsvCtt") === "보존") {
            txt_titPrsvCtt.setValue("보존여부");
            txt_prsvCtt.setValue("기록보존됨");
        } else if(dma_csBasCtt.get("prsvCtt") === "폐기") {
            txt_titPrsvCtt.setValue("폐기여부");
            txt_prsvCtt.setValue("기록폐기됨");
        }
        tr_prsvCtt.show("");
    } else  {
        tr_prsvCtt.hide();
    }
}]]></script>
