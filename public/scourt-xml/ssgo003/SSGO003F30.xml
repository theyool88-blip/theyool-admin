scwin.onpageload = function() {
    var dlt_param   = com.data.getParameter() || ssgo.getParameter();
    dlt_param = ssgo.setReversLst(dlt_param);

    dlt_rcntDxdyLst.removeAll();
    dlt_rcntDxdyLst.setJSON(dlt_param);    

    // 조회건수 없을시 버튼숨김
    if(dlt_rcntDxdyLst.getRowCount() == 0) {
        btn_csDxsyProgList.hide();
    }    

    // 그리드 사이즈조정시 캔버스뒤로 숨는 오류처리
    ssgo.setGrdResizeAddEvt(grd_rcntDxdyLst);       

};

/**
