{
  "crons": [
    {
      "path": "/api/cron/daily-reminders",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/scourt-sync-scheduler?secret=scourt-batch-sync-secret",
      "schedule": "*/10 * * * *"
    },
    {
      "path": "/api/cron/scourt-sync-worker?secret=scourt-batch-sync-secret",
      "schedule": "*/2 * * * *"
    },
    {
      "path": "/api/cron/batch-import-worker?secret=batch-import-secret",
      "schedule": "* * * * *"
    }
  ]
}
